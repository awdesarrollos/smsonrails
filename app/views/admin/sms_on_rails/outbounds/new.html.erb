<h1>New outbound</h1>

<% form_for(:outbound, @outbound, :url => sms_outbounds_path, :html => {:method => :put }) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_field :notes %>
  </p>
  <p>
    <%= f.label :processed_on %><br />
    <%= f.datetime_select :processed_on %>
  </p>
  <p>
    <%= f.label :sms_service_provider_id %><br />
    <%= f.text_field :sms_service_provider_id %>
  </p>
  <p>
    <%= f.label :created_at %><br />
    <%= f.datetime_select :created_at %>
  </p>
  <p>
    <%= f.label :retry_count %><br />
    <%= f.text_field :retry_count %>
  </p>
  <p>
    <%= f.label :sub_status %><br />
    <%= f.text_field :sub_status %>
  </p>
  <p>
    <%= f.label :options %><br />
    <%= f.text_field :options %>
  </p>
  <p>
    <%= f.label :send_priority %><br />
    <%= f.text_field :send_priority %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', sms_outbounds_path %>